{
  "compilerOptions": {
    "checkJs": true,

    "charset": "utf8",
    "emitBOM": false,

    "module": "es2015",
    "target": "es5",
    "outDir": "target/vscode/",
    "sourceMap": true,
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    "target",
    "**/target/*"
  ]
}

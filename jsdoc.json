{
  "tags": {
    "allowUnknownTags":     true
  },
  "opts": {
    "destination":          "./target/docs"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks":          true,
    "monospaceLinks":       false,
    "default": {
      "outputSourceFiles":  true,
      "staticFiles": {
        "include":          []
      }
    },
    "path":                 "ink-docstrap",
    "theme":                "cerulean",
    "navType":              "vertical",
    "linenums":             true,
    "dateFormat":           "MMMM Do YYYY, h:mm:ss a"
  }
}